
# Ctrl  : C-, ^
# Escape: ESC-, M-, ^[
# Space : SPC, ' '
# Tab   : TAB, ^i, ^I
# Delete: DEL, ^?
# Up    : UP, ^[[A
# Down  : DOWN, ^[[B
# Right : RIGHT, ^[[C
# Left  : LEFT, ^[[D


### MARK ###
keymap    v      MARK
keymap    m      MARK_WORD
keymap    M-n    NEXT_MARK
keymap    M-p    PREV_MARK


### WORD ###
keymap    B      PREV_WORD
keymap    b      NEXT_WORD


### LINE ###
keymap    I      LINE_BEGIN
keymap    A      LINE_END
keymap    C-g    GOTO_LINE


### HISTORY ###
keymap    h      HISTORY


### TAB ###
keymap    GT     PREV_TAB
keymap    gt     NEXT_TAB
keymap    T      NEW_TAB
keymap    C-t    TAB_LINK
keymap    q      CLOSE_TAB


### LINK ###
keymap    (      LINK_BEGIN
keymap    )      LINK_END
keymap    w      SAVE_LINK
keymap    O      GOTO_LINK
keymap    L      PREV_LINK
keymap    l      NEXT_LINK


### SHELL ###
keymap    !      SHELL
keymap    @      READ_SHELL
keymap    ::     COMMAND


### NAVIGATION ###
keymap    u           UNDO
keymap    C-r         REDO

keymap    <           SHIFT_LEFT
keymap    >           SHIFT_RIGHT

keymap    C-u          BACK

keymap    Z           CENTER_H
keymap    z           CENTER_V

keymap    UP          MOVE_UP
keymap    DOWN        MOVE_DOWN
keymap    RIGHT       MOVE_RIGHT
keymap    LEFT        MOVE_LEFT

keymap    r           RELOAD
keymap    R           NEXT_PAGE
keymap    U           PREV_PAGE

keymap    J           UP
keymap    K           DOWN

keymap    G           END
keymap    g           BEGIN

keymap    O           GOTO


### SEARCH ###
keymap    /      SEARCH
keymap    N      SEARCH_PREV
keymap    C-n    SEARCH_NEXT


### IMAGES  ###
keymap    i    PEEK_IMG
keymap    vi   VIEW_IMAGE
keymap    :i   SAVE_IMAGE


### ACTIONS ###
keymap    :w     SAVE
keymap    C-l    LIST
keymap    y      EXTERN
keymap    :l     LOAD
keymap    s      SELECT_MENU
keymap    :v     VIEW
keymap    c      PEEK


### BOOKMARK ###
keymap    C-a    ADD_BOOKMARK
keymap    C-b    BOOKMARK


### SCREEN ###
keymap    e      EDIT_SCREEN
keymap    w      SAVE_SCREEN


### MENU ###
keymap    =      INFO
keymap    D      DOWNLOAD_LIST
keymap    E      EDIT
keymap    :h     HELP

keymap    C-M    MENU
keymap    M-l    LIST_MENU
keymap    M-m    MOVE_LIST_MENU

keymap    :o     OPTIONS

###  EXIT ###
keymap    :q     EXIT
keymap    :q!    QUIT
